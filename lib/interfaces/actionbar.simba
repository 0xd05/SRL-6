function TRSActionBar.__getMinimizeButtonBounds(opened: boolean): TBox;
begin
  result := [self.closedBounds.x2-16, self.closedBounds.y1, self.closedBounds.x2, self.closedBounds.y1 + 16];

  if (opened) then
    result.offset([0, -38]);
end;

function TRSActionBar.__findButton(area: TBox): boolean;
var
  wid, hei: integer;
  tpa: TPointArray;
begin
  if (findColorsTolerance(tpa, 10722966, area, 11)) then
  begin
    getTPABounds(tpa).getDimensions(wid, hei);
    result := ((wid = 9) and (hei = 2));
  end;
end;

